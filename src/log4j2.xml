<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<File name="log" fileName="./errors.txt" append="false">
			<PatternLayout pattern="%d{yyyy/MM/dd HH:mm:ss.SSS}-%m%n"/>
		</File>
	</Appenders>
	
	<loggers>
		<root level="ERROR">
			<appender-ref ref="log"/>
		</root>
	</loggers>
</Configuration>